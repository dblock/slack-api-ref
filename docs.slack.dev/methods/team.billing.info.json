{
  "desc": "Reads a workspace's billing plan information.",
  "http_method": "POST",
  "scope": "team.billing:read",
  "rate_limits": "t3",
  "args": {
    "type": "object",
    "required": [
      "token"
    ],
    "properties": {
      "token": {
        "type": "string",
        "subtype": "auth",
        "desc": "Authentication token bearing required scopes. Tokens should be passed as an HTTP Authorization header or alternatively, as a POST parameter.",
        "example": "xxxx-xxxxxxxxx-xxxx"
      }
    }
  },
  "output": {
    "type": "object",
    "required": [
      "plan"
    ],
    "properties": {
      "plan": {
        "type": "string"
      }
    }
  },
  "errors": {
    "invalid_auth": {
      "desc": "Invalid token provided."
    },
    "invalid_team": {
      "desc": "Cannot find a team associated with the authed token."
    }
  },
  "examples": {
    "success": {
      "url": "/api/team.preferences.list",
      "description": "Typical success response",
      "example": {
        "ok": true,
        "plan": "free"
      }
    },
    "error": {
      "url": "/api/team.profile.get?token=INVALID_TOKEN",
      "description": "Typical error response",
      "example": {
        "ok": false,
        "error": "invalid_team"
      }
    }
  }
}