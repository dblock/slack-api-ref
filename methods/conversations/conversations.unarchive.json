{
  "group": "conversations",
  "name": "conversations.unarchive",
  "deprecated": false,
  "desc": "Reverses conversation archival.",
  "args": {
    "channel": {
      "required": true,
      "desc": "ID of conversation to unarchive.",
      "type": "channel"
    }
  },
  "response": {
    "examples": [
      "{\n    \"ok\": true\n}",
      "{\n    \"ok\": false,\n    \"error\": \"channel_not_found\"\n}"
    ]
  },
  "errors": {
    "channel_not_found": "Value passed for `channel` was invalid.",
    "method_not_supported_for_channel_type": "This type of conversation cannot be used with this method.",
    "missing_scope": "The calling token is not granted the necessary scopes to complete this operation.",
    "not_archived": "Channel is not archived."
  }
}