{
  "group": "canvases",
  "name": "canvases.delete",
  "deprecated": false,
  "desc": "Deletes a canvas",
  "args": {
    "canvas_id": {
      "required": true,
      "example": "F1234ABCD",
      "desc": "Encoded ID of the canvas."
    }
  },
  "response": {
    "examples": [
      "{\n    \"ok\": true\n}",
      "{\n    \"ok\": false,\n    \"error\": \"canvas_not_found\"\n}"
    ]
  },
  "errors": {
    "canvas_deleting_disabled": "This functionality is disabled.",
    "canvas_not_found": "The canvas does not exist, or is not visible to the calling user.",
    "restricted_action": "User does not have permission to perform this action."
  }
}