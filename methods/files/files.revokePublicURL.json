{
  "group": "files",
  "name": "files.revokePublicURL",
  "deprecated": false,
  "desc": "Revokes public/external sharing access for a file",
  "args": {
    "file": {
      "required": true,
      "desc": "File to revoke.",
      "type": "file"
    }
  },
  "response": {
    "examples": [
      "{\n    \"ok\": true\n}",
      "{\n    \"ok\": false,\n    \"error\": \"invalid_auth\"\n}"
    ]
  },
  "errors": {
    "file_not_found": "Value passed for `file` was invalid"
  }
}