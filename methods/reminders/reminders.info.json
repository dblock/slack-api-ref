{
  "group": "reminders",
  "name": "reminders.info",
  "deprecated": false,
  "desc": "Gets information about a reminder.",
  "args": {
    "reminder": {
      "required": true,
      "example": "Rm23456789",
      "desc": "The ID of the reminder.",
      "type": "string"
    },
    "team_id": {
      "required": false,
      "desc": "Encoded team id, required if org token is passed.",
      "type": "string"
    }
  },
  "response": {
    "examples": [
      "{\n    \"ok\": true\n}",
      "{\n    \"ok\": false,\n    \"error\": \"invalid_auth\"\n}"
    ]
  },
  "errors": {
    "missing_argument": "An argument is missing.",
    "not_found": "That reminder can't be found."
  }
}